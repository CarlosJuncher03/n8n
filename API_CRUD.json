      "parameters": {
        "operation": "executeQuery",
        "query": "SELECT\n    200 AS CODIGO,\n    \"CADASTRO ENCONTRADO\" AS MENSAGEM,\n    ID,\n    ID_CONTRATANTE,\n    NOME_EMPRESA,\n    CNPJ_CPF,\n    UF,\n    CIDADE,\n    BAIRRO,\n    RUA,\n    NUMERO,\n    EMAIL,\n    MATRIZ,\n    ID_MATRIZ,\n    UUID,\n    ESPORADICO,\n    TIPO,\n    DATA_CRIADO,\n    ATIVO,\n    CEP,\n    COMPLEMENTO\nFROM\n    HELP_DESK_V1.EMPRESA \nWHERE \n    CNPJ_CPF = \"{{ $json.cnpj_cpf }}\"\n\nUNION ALL\n\nSELECT \n    204 AS CODIGO,\n    \"CADASTRO N√ÉO ENCONTRADO\" AS MENSAGEM,\n    NULL AS ID,\n    NULL AS ID_CONTRATANTE,\n    NULL AS NOME_EMPRESA,\n    NULL AS CNPJ_CPF,\n    NULL AS UF,\n    NULL AS CIDADE,\n    NULL AS BAIRRO,\n    NULL AS RUA,\n    NULL AS NUMERO,\n    NULL AS EMAIL,\n    NULL AS MATRIZ,\n    NULL AS ID_MATRIZ,\n    NULL AS UUID,\n    NULL AS ESPORADICO,\n    NULL AS TIPO,\n    NULL AS DATA_CRIADO,\n    NULL AS ATIVO,\n    NULL AS CEP,\n    NULL AS COMPLEMENTO\nFROM DUAL\nWHERE NOT EXISTS (\n    SELECT 1 FROM HELP_DESK_V1.EMPRESA WHERE CNPJ_CPF = \"{{ $json.cnpj_cpf }}\"\n);\n",
        "options": {}
      },
      "type": "n8n-nodes-base.mySql",
      "typeVersion": 2.4,
      "position": [
        -80,
        40
      ],
      "id": "c39b17a4-f36d-44e6-a8a3-a9adf3917ea9",
